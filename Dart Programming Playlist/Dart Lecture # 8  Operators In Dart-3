//Dart Lecture # 8  Operators In Dart-3

void main() {
  //Assignment operators

  var a = 2; //Assignment Operator
  
  print(a+=2);    //Compound assignment  +=
  print(a-=2);    //Compound assignment  -=
  print(a*=2);    //Compound assignment  *=
  print(a~/=1);   //Compound assignment  ~/=
  

  
  //Conditional expression
  
   int n1 = 5, n2 = 10;

    // Largest among n1 and n2 
    var max = (n1 > n2) ? n1 : n2; 
    print(max);
}


/*
Console Output

4
2
4
4
10

*/
